{
  "name": "prajapatidhara1510/laravel-prestashop-webservice",
  "description": "Laravel 12 wrapper for Prestashop Web Service Library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "prestashop",
    "webservice",
    "api"
  ],
  "authors": [
    {
      "name": "Dhara Pawar",
      "email": "prajapatidhara@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PrajapatiDhara1510\\PrestashopWebService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PrajapatiDhara1510\\PrestashopWebService\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PrajapatiDhara1510\\PrestashopWebService\\PrestashopWebServiceProvider"
      ],
      "aliases": {
        "Prestashop": "PrajapatiDhara1510\\PrestashopWebService\\PrestashopWebServiceFacade"
      }
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.2",
    "orchestra/testbench": "^3.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs src/*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}